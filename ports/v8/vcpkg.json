{
  "name": "v8",
  "version-date": "2026-01-13",
  "port-version": 2,
  "description": "V8 JavaScript Engine (CMake wrapper via v8-cmake)",
  "homepage": "https://v8.dev/",
  "license": "BSD-3-Clause",
  "supports": "!uwp & !android",
  "dependencies": [
    {
      "name": "vcpkg-cmake",
      "host": true
    },
    {
      "name": "vcpkg-cmake-config",
      "host": true
    }
  ],
  "default-features": [
    "monolithic"
  ],
  "features": {
    "monolithic": {
      "description": "Build a single monolithic library instead of separate components"
    }
  }
}