{
  "name": "mlspp",
  "version-date": "2026-01-08",
  "description": "Cisco MLS C++ library",
  "dependencies": [
    "catch2",
    "nlohmann-json",
    {
      "name": "vcpkg-cmake",
      "host": true
    },
    {
      "name": "vcpkg-cmake-config",
      "host": true
    }
  ],
  "features": {
    "openssl": {
      "description": "Build OpenSSL from source (disable for system OpenSSL)",
      "dependencies": [
        {
          "name": "openssl",
          "version>=": "3.0.7"
        }
      ]
    }
  },
  "overrides": [
    {
      "name": "openssl",
      "version": "3.0.7"
    }
  ]
}